alias.st=status -s
alias.cl=clone
alias.ci=commit
alias.cm=commit -m
alias.cma=commit -a -m
alias.ca=commit --amend
alias.amend=commit --amend
alias.caa=commit -a --amend -C HEAD
alias.filelog=log -u
alias.fl=log -u
alias.ai=add --interactive
alias.co=checkout
alias.br=branch
alias.bra=branch -ra
alias.le=log --oneline --decorate
alias.ll=log --pretty=format:%C(yellow)%h%Cred%d\ %Creset%s%Cred\ [%cn] --decorate --numstat
alias.ls1=log --pretty=format:%C(yellow)%h%Cred%d\ %Creset%s%Cred\ [%cn] --decorate
alias.lds=log --pretty=format:%C(yellow)%h\ %C(green)%ad%Cred%d\ %Creset%s%Cred\ [%cn] --decorate --date=short --graph
alias.ls=log --pretty=format:%C(green)%h\ %C(yellow)[%ad]%Cred%d\ %Creset%s%Cred\ [%cn] --decorate --date=relative
alias.lc=!f() { git ll $1^..$1; }; f
alias.lnc=log --pretty=format:%h\ %s\ [%cn]
alias.la=!git config -l | grep alias | cut -c 7-
alias.diff=diff --word-diff
alias.d=diff --word-diff
alias.dc=diff --cached
alias.dl=!git ll -1
alias.dlc=diff --cached HEAD^
alias.dr=!f() { git diff -w $1^..$1; }; f
alias.diffr=!f() { git diff $1^..$1; }; f
alias.branch=branch -ra
alias.r=reset
alias.r1=reset HEAD^
alias.r2=reset HEAD^^
alias.rh=reset --hard
alias.rh1=reset HEAD^ --hard
alias.rh2=reset HEAD^^ --hard
alias.svnr=svn rebase
alias.svnd=svn dcommit
alias.svnl=svn log --oneline --show-commit
alias.sl=stash list
alias.sa=stash apply
alias.ss=stash save
alias.cp=cherry-pick
alias.grep=grep -Ii
alias.gr=grep -Ii
alias.gra=!f() { A=$(pwd) && TOPLEVEL=$(git rev-parse --show-toplevel) && cd $TOPLEVEL && git grep --full-name -In $1 | xargs -I{} echo $TOPLEVEL/{} && cd $A; }; f
alias.f=!git ls-files | grep -i
alias.done=!f() { git branch | grep $1 | cut -c 3- | grep -v done | xargs -I{} git branch -m {} done-{}; }; f
alias.assume=update-index --assume-unchanged
alias.unassume=update-index --no-assume-unchanged
alias.assumed=!git ls-files -v | grep ^h | cut -c 3-
alias.unassumeall=!git assumed | xargs git update-index --no-assume-unchanged
alias.assumeall=!git st -s | awk {'print $2'} | xargs git assume
alias.lasttag=describe --tags --abbrev=0
alias.lt=describe --tags --abbrev=0
alias.ours=!f() { git co --ours $@ && git add $@; }; f
alias.theirs=!f() { git co --theirs $@ && git add $@; }; f
alias.rem=!git config -l | grep remote.*url | tail -n +2
alias.lap=!f() { git --git-dir=$1 format-patch -k -1 --stdout $2 | git am -3 -k; }; f
alias.sba=!f() { git subtree add --prefix $2 $1 master --squash; }; f
alias.empty=!git commit -am"[empty] Initial commit" --allow-empty
